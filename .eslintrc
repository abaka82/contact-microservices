{
  extends: eslint:recommended,
  rules: {
    no-console: 0,
    indent: [2, 2, {"SwitchCase": 1}],
    no-multi-spaces: 2,
    no-underscore-dangle: 0,
    no-use-before-define: [1, "nofunc"],
    no-unused-expressions: 0,
    no-empty-class: 0,
    object-curly-spacing: [2, "always"],
    space-in-parens: [2, "never"]
  },
  env: {
    node: true,
    es6: true
  }
}
